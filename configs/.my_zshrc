export LC_ALL=en_US.UTF-8
export LANG=en_US.UTF-8
export LANGUAGE=en_US.UTF-8

plugins=(
	git
	fzf
	docker
	zsh-wakatime
	docker-compose
	zsh-autosuggestions
	zsh-syntax-highlighting
)

alias ls="eza -la"
alias cat="bat -P"
alias start_colima="colima start -c 4 -m 8 -d 100"

from_base_64() {
  echo "$1" | base64 -d | jq
}